{
    "name": "Web Tracker",
    "description": "Track pages and tags visited",
    "version": "0.1",
    "manifest_version": 2,
    "incognito": "split",
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "js": ["main.js"],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
    ]
}